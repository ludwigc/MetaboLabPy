# 47 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
"acqt0=0"
baseopt_echo


# 1 "mc_line 51 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) Echo-Antiecho
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define delay d10orig
"d10orig=d10"
define delay d1orig
"d1orig=d1"
define list<loopcounter> SPARSELIST = <$NUSLIST>
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter STNCNT
"STNCNT=SPARSELIST.len/1"
"MCREST = d11 - d11"
"MCWRK = 0.052632*d11"
# 51 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
1 ze
# 1 "mc_line 51 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl exp. def. of mc cmd. after ze"
LBLAV, MCWRK
  MCWRK
  "t1loop = SPARSELIST"
  MCWRK  SPARSELIST.inc
  MCWRK
  "ph1loop = 0"
      "d0=d0orig + t1loop * in0 "
      "d10=d10orig + t1loop * in10 "
      "d1=d1orig - t1loop * in1 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
# 52 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
  d11 pl12:f2
# 1 "mc_line 53 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl exp. start label for mc cmd."
2 MCWRK  * 7 do:f2
LBLF1, MCWRK  * 11
LBLSTN, MCWRK 
  MCREST
# 54 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
  ;d12
  d12 pl9:f1
  d1  cw:f1 ph29
  d12 do:f1
  d12 pl1:f1 
3 (p1 ph1):f1
  DELTA2 pl0:f2
  4u
  (center (p2 ph1) (p14:sp3 ph6):f2 )
  4u
  DELTA2 pl2:f2 setrtp0|0 ctrlgrad 0
;  p28 ph1
;  4u
  (p1 ph2):f1
;  d12 pl0:f1
;  (p11:sp1 ph1):f1
  d12 ;pl1:f1
  p16:gp3*EA
# 75 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
  d16

  (p3 ph3):f2
;  d10
;# ifdef LABEL_CN
;  (center (p24:sp7 ph4):f2 (p22 ph1):f3)
;# else
;  (p24:sp7 ph4):f2
;# endif
;  d10
;  3u
;  (p24:sp7 ph4):f2
;  3u
  d0 

;#   ifdef LABEL_CN
;  (center (p2 ph5) (p22 ph1):f3 )
;#   else
  (p2 ph5)
;#   endif 
# 96 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
  d0
  p16:gp1*EA
  d16 pl0:f2
  d10


# 103 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
  (p24:sp7 ph4):f2

  d10
  DELTA pl2:f2
  (p3 ph4):f2
  d12
  p16:gp4*EA
  d16
  (p1 ph1):f1
  DELTA2 pl0:f2
  (center (p2 ph1) (p14:sp3 ph1):f2 )
  4u
  p16:gp2
  DELTA1 pl12:f2
  4u setrtp0^0 ctrlgrad 4047
  go=2 ph31 cpd2:f2
# 1 "mc_line 119 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl exp. mc cmd. in line"
  MCWRK  do:f2 wr #0 if #0 zd 

  MCWRK
  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "d10=d10orig + t1loop * in10 "
      "d1=d1orig - t1loop * in1 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLF1 times 2
  MCWRK 
  MCWRK
  "ph1loop-=2"
  MCWRK 

  MCWRK
  "t1loop = SPARSELIST"
  MCWRK  SPARSELIST.inc
  MCWRK
  "ph1loop = 0"
      "d0=d0orig + t1loop * in0 "
      "d10=d10orig + t1loop * in10 "
      "d1=d1orig - t1loop * in1 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLSTN times STNCNT
  MCWRK rf #0 SPARSELIST.res
  lo to LBLAV times tdav
# 121 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
exit
   

ph1=0 
ph2=1
ph3=0 2
ph4=0 0 0 0 2 2 2 2
ph5=0 0 2 2
ph6=0
ph29=0
ph31=0 2 0 2 2 0 2 0
# 134 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
;pl0 : 120dB
;pl1 : f1 channel - power level for pulse (default)
;pl2 : f2 channel - power level for pulse (default)
;pl3 : f3 channel - power level for pulse (default)
;pl12: f2 channel - power level for CPD/BB decoupling
;sp3: f2 channel - shaped pulse 180 degree for inversion
;sp7: f2 channel - shaped pulse 180 degree for refocussing
;p1 : f1 channel -  90 degree high power pulse
;p2 : f1 channel - 180 degree high power pulse
;p3 : f2 channel -  90 degree high power pulse
;p14: f2 channel - 180 degree shaped pulse for inversion
;p16: homospoil/gradient pulse
;p22: f3 channel - 180 degree high power pulse
;p24: f2 channel - 180 degree shaped pulse for refocussing
;p28: f1 channel - trim pulse
;d0 : incremented delay (2D)                         [3 usec]
;d1 : relaxation delay; 1-5 * T1
;d4 : 1/(4J)XH
;d11: delay for disk I/O                             [30 msec]
;d16: delay for homospoil/gradient recovery
;cnst2: = J(XH)
;inf1: 1/SW(X) = 2 * DW(X)
;in0: 1/(2 * SW(X)) = DW(X)
;nd0: 2
;NS: 1 * n
;DS: >= 16
;td1: number of experiments
;FnMODE: echo-antiecho
;cpd2: decoupling according to sequence defined by cpdprg2
;pcpd2: f2 channel - 90 degree pulse for decoupling sequence


;use gradient ratio:	gp 1 : gp 2
;			  80 : 20.1    for C-13
;			  80 :  8.1    for N-15
# 170 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
;for z-only gradients:
;gpz1: 80%
;gpz2: 20.1% for C-13, 8.1% for N-15

;use gradient files:   
;gpnam1: SINE.100
;gpnam2: SINE.100

# 179 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/jres_hsqcetgpsp_vrt.22.cl"
